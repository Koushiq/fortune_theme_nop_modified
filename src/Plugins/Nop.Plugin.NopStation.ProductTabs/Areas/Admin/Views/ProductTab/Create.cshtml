@model ProductTabModel

@{
    Layout = "_AdminLayout";
    //page title
    ViewBag.PageTitle = T("NopStation.ProductTabs.ProductTabs.AddNew").Text;
    //active menu item (system name)
    Html.SetActiveMenuItemSystemName("ProductTabs");
}

<form asp-controller="ProductTab" asp-action="Create" method="post" id="home-page-slider-form">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("NopStation.ProductTabs.ProductTabs.AddNew")
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="List">@T("NopStation.ProductTabs.ProductTabs.BackToList")</a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdate", Model)
</form>